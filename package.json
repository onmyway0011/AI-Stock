{
  "name": "ai-stock-trading-system",
  "version": "1.0.0",
  "description": "Advanced AI-powered stock trading system with real-time data collection, intelligent strategies, and comprehensive backtesting",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "demo:data": "ts-node examples/data-collection-demo.ts",
    "demo:strategies": "ts-node examples/advanced-strategy-demo.ts",
    "demo:backtest": "ts-node examples/run-demo.ts",
    "backtest": "ts-node scripts/run-backtest.ts",
    "backtest:basic": "ts-node examples/historical-backtest-demo.ts",
    "backtest:demo": "ts-node examples/historical-backtest-demo.ts",
    "backtest:optimize": "ts-node scripts/run-backtest.ts optimize",
    "backtest:compare": "ts-node scripts/run-backtest.ts compare",
    "backtest:cache": "ts-node scripts/run-backtest.ts cache",
    "precommit": "npm run lint && npm run test",
    "postinstall": "npm run build",
    "docker:build": "docker build -t ai-stock-trading .",
    "docker:run": "docker run -p 3000:3000 ai-stock-trading",
    "docker:dev": "docker-compose up --build"
  },
  "keywords": [
    "trading",
    "ai",
    "stock",
    "cryptocurrency",
    "binance",
    "strategy",
    "backtest",
    "technical-analysis",
    "machine-learning",
    "typescript"
  ],
  "author": "AI Trading Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mathjs": "^12.2.0",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "redis": "^4.6.0",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-stock-trading-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-stock-trading-system/issues"
  },
  "homepage": "https://github.com/yourusername/ai-stock-trading-system#readme"
}
